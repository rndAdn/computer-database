<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">

	<servlet>
		<servlet-name>Dashboard</servlet-name>
		<servlet-class>com.excilys.computerdatabase.computerdb.ui.web.Dashboard</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Dashboard</servlet-name>
		<url-pattern>/dashboard</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>EditComputer</servlet-name>
		<servlet-class>com.excilys.computerdatabase.computerdb.ui.web.EditComputer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EditComputer</servlet-name>
		<url-pattern>/editComputer</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AddComputer</servlet-name>
		<servlet-class>com.excilys.computerdatabase.computerdb.ui.web.AddComputer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddComputer</servlet-name>
		<url-pattern>/addComputer</url-pattern>
	</servlet-mapping>
</web-app>
